<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android">

<layer-list>
    <item>
    <shape
        android:shape="rectangle">
        <solid android:color="#ffffff"/>

        <size android:width="70dp"
            android:height="26dp"/>
        <corners android:radius="20dp"/>
